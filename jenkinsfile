pipeline
{
agent any 
tools { 
        maven 'Maven 3.6.3' 
        jdk 'jdk1.8.0_202' 
    }

  stages {
 
       stage ('git checkout') {
	   
	      steps {
		  
		   git branch: 'develop', url: 'https://github.com/mrudulapolavaram/devopstrainrepo.git'
		   
		   }
		}
		   
		   
		stage ('Initialize') {
            steps {
                sh '''
                    echo "PATH = ${PATH}"
                    echo "M2_HOME = ${M2_HOME}"
                ''' 
            }
	    
	} 
	
}  
}