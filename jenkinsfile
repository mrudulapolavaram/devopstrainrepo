pipeline
{
agent any 

  stages {

     stage('compile') {
   
         steps {
 
    		build 'pipeline-compile'
	       }
    
       		   }
	stage('test') {
    
           steps {

		build 'pipeline-test'
		
		}
		    }

	stage('deploy') {

	   steps {

		build 'pipeline-install'

		}

		    }
	} 
	
}  