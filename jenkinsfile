pipeline
{
agent any 

  stages {
 
       stage ('git checkout') {
	   
	      steps {
		  
		   git branch: 'develop', url: 'https://github.com/mrudulapolavaram/devopstrainrepo.git'
		   
		   }
		}
		   
	    stage ('compile') {
		
		  steps {
		  
		   build 'firstjob'
		   
		   }
		}
	} 
	
}  