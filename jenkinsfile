pipeline
{
agent { label 'node1' 
   dockerfile true
   }
tools { 
        maven 'MAVEN363' 
        jdk 'java1.8.0' 
    }

  stages {
 
       stage ('git checkout') {
	   
	      steps {
		  
		   git branch: 'develop', url: 'https://github.com/mrudulapolavaram/devopstrainrepo.git'
		   
		   }
		}
		   
		   
		stage ('Initialize') {
            steps {
                    echo "PATH = ${PATH}"
                    echo "M2_HOME = ${M2_HOME}"
                 
          }
		 
	    
	} 
	stage ('Build') {
            steps {
           				    sh 'mvn clean compile'
			}
            }
	stage ('testing') {
            steps {
           				    sh 'mvn test'
			}
			}
	stage ('install') {
            steps {
           				    sh 'mvn install'
			}
            }	
            
            }
			
}  
